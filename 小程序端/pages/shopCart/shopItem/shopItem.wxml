<view class="shopItem data-v-5adf3b3f"><block wx:for="{{cartdata}}" wx:for-item="shop" wx:for-index="shopIndex" wx:key="shopIndex"><block class="data-v-5adf3b3f"><view class="item data-v-5adf3b3f"><view class="item-title data-v-5adf3b3f"><image lazy-load="{{true}}" src="/static/images/createdorder/shop.png" class="data-v-5adf3b3f"></image><view class="item-shopname data-v-5adf3b3f">{{shop.supplier_name}}</view><block wx:if="{{shop.supplier_id==0}}"><view class="item-label data-v-5adf3b3f">自营</view></block></view><view class="data-v-5adf3b3f"><block wx:for="{{shop.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="data-v-5adf3b3f"><view class="cartData-item data-v-5adf3b3f"><block wx:if="{{item.goods_status==1}}"><checkbox data-index="{{index}}" data-shopindex="{{shopIndex}}" checked="{{item.flag}}" data-event-opts="{{[['tap',[['change',['$event']]]]]}}" bindtap="__e" class="data-v-5adf3b3f"></checkbox></block><block wx:else><view class="faill data-v-5adf3b3f">失效</view></block><view class="shop-left data-v-5adf3b3f"><image class="cartData-img data-v-5adf3b3f" data-url="{{'/pages/goodsItem/goodsItem/goodsItem?id='+item.goods_id}}" lazy-load="{{true}}" src="{{item.pic_url}}" data-event-opts="{{[['tap',[['goItem',['$event']]]]]}}" catchtap="__e"></image><block wx:if="{{item.goods_status==0}}"><view data-id="{{item.id}}" data-event-opts="{{[['tap',[['delGoods',['$event']]]]]}}" bindtap="__e" class="data-v-5adf3b3f">清除失效宝贝</view></block></view><view class="cartData-content data-v-5adf3b3f"><view class="cartData-name data-v-5adf3b3f">{{item.goods_name}}</view><view class="cartData-property data-v-5adf3b3f">{{item.property1_name+","+item.property2_name}}</view><view class="space-between data-v-5adf3b3f"><view class="cartData-price data-v-5adf3b3f">{{"￥"+item.price}}</view><view class="cartData-tabbar data-v-5adf3b3f"><view class="cartData-tabbar-btn data-v-5adf3b3f" data-index="{{index}}" data-shopindex="{{shopIndex}}" data-stock_id="{{item.stock_id}}" data-goods_id="{{item.goods_id}}" data-type="reduce" data-event-opts="{{[['tap',[['changeCartData',['$event']]]]]}}" bindtap="__e">-</view><view class="cartData-tabbar-nums data-v-5adf3b3f">{{item.number}}</view><view class="cartData-tabbar-btn data-v-5adf3b3f" shopindex="{{true}}" data-index="{{index}}" data-shopindex="{{shopIndex}}" data-stock_id="{{item.stock_id}}" data-goods_id="{{item.goods_id}}" data-type="add" data-event-opts="{{[['tap',[['changeCartData',['$event']]]]]}}" bindtap="__e">+</view></view></view></view></view></block></block></view></view></block></block></view>